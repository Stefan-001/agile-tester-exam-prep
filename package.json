    "papaparse": "^5.4.1",
    "pdfjs-dist": "^4.6.82",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "pdf-parse": "^1.1.1",
